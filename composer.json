{
    "name": "t3/mbox",
    "description": "TYPO3 CMS backend module to view mbox file contents, like an email client.",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/a-r-m-i-n/mbox",
    "support": {
        "issues": "https://github.com/a-r-m-i-n/mbox/issues",
        "source": "https://github.com/a-r-m-i-n/mbox"
    },
    "authors": [
        {
            "name": "Armin Vieweg",
            "email": "armin@v.ieweg.de",
            "homepage": "https://v.ieweg.de"
        }
    ],
    "keywords": [
        "typo3",
        "mbox",
        "email",
        "mail",
        "viewer",
        "mail viewer"
    ],
    "require": {
        "php": "^8.2",
        "typo3/cms-core": "^12.4 || ^13",
        "armin/mbox-parser": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "T3\\Mbox\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "mbox"
        }
    }
}
